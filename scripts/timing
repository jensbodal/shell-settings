#!/bin/bash
# source: https://unix.stackexchange.com/a/334152

[ -z $SHELL_SETTINGS_INIT ] && source $HOME/github/shell-settings/scripts/.INIT

START=$(date +%s.%N)
# do something #######################

tmp=`mktemp`
eval "${@}" > $tmp 2>&1

#######################################
END=$(date +%s.%N)
DIFF=$( echo "scale=3; (${END} - ${START})*1000/1" | bc )

echo "[${DIFF}ms | \`${@}\`] `cat ${tmp}`"
